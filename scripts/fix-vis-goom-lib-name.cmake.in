set(GIVEN_VIS_GOOM_LIB_NAME "${CMAKE_INSTALL_PREFIX}/lib@VIS_GOOM_TARGET_NAME@.so")
set(REQUIRED_VIS_GOOM_LIB_NAME
    "${CMAKE_INSTALL_PREFIX}/@VIS_GOOM_TARGET_NAME@.so.@VIS_GOOM_ADDON_VERSION@")

file(RENAME ${GIVEN_VIS_GOOM_LIB_NAME} ${REQUIRED_VIS_GOOM_LIB_NAME})

if(EXISTS ${REQUIRED_VIS_GOOM_LIB_NAME})
  message(STATUS "Installing: Renamed lib to \"${REQUIRED_VIS_GOOM_LIB_NAME}\"")
else()
  message(FATAL "Installing: Could not find lib file \"${REQUIRED_VIS_GOOM_LIB_NAME}\"")
endif()
